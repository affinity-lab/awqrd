{
  "name": "@affinity-lab/util",
  "version": "2.0.205",
  "description": "Utils for Comet and Storm",
  "author": "Affinity Works",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./types/index.d.ts",
      "default": "./dist/index.js"
    },
    "./frontend": {
      "import": "./dist/index-frontend.js",
      "types": "./types/index-frontend.d.ts",
      "default": "./dist/index-frontend.js"
    }
  },
  "files": [
    "dist",
    "src",
    "types",
    "tsconfig.json"
  ],
  "scripts": {
    "prebuild": "bunx rimraf dist && bunx rimraf types",
    "build": "bunx tsc",
    "postbuild": "bunx ts-add-js-extension --dir=dist"
  },
  "dependencies": {
    "@types/mime-types": "^2.1.4",
    "@types/nodemailer": "^6.4.15",
    "chalk": "^5.3.0",
    "change-case-all": "^2.1.0",
    "fast-glob": "^3.3.2",
    "jsonwebtoken": "^9.0.2",
    "mime-types": "^2.1.35",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.9.13",
    "memcached": "^2.2.2",
    "sharp": "^0.33.3",
    "sql-highlight": "^4.4.2",
    "uni-flatten": "^1.6.2",
    "redis": "^4.7.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^20.12.8",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/memcached": "^2.2.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "repository": {
    "url": "git://github.com/affinity-lab/awqrd.git"
  }
}